   <service name="createQualification" engine="java" default-entity-name="PartyQual" location="org.ofbiz.humanres.PartyQual" invoke="createQualification" auth="true">
		<description>Create a Qualification</description>
		<permission-service service-name="humanResManagerPermission" main-action="CREATE" />
		<attribute name="partyId" mode="IN" type="String" optional="true"/>
		<attribute name="employeeId" mode="IN" type="String" optional="false"/>
		<attribute name="partyQualTypeId" mode="IN" type="String" optional="true"/>
