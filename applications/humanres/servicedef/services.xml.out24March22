		<attribute name="passingYear" mode="IN" type="Long" optional="true"/>
		<attribute name="title" mode="IN" type="String" optional="true"/>
		<attribute name="statusId" mode="IN" type="String" optional="true"/>
		<attribute name="fromDate" mode="IN" type="Timestamp" optional="true"/>
		<attribute name="countryGeoId" mode="IN" type="String" optional="true"/>
		<attribute name="remarks" mode="IN" type="String" optional="true"/>
	</service>
	<service name="createContessaWeekHoliday" engine="java" location="org.ofbiz.humanres.LeaveEvents" invoke="createContessaWeekHoliday" auth="true">
		<description>Create a Qualification</description>
		<permission-service service-name="humanResManagerPermission" main-action="CREATE" />
		<attribute name="dayName" mode="INOUT" type="String" optional="false"/>
		<attribute name="companyId" mode="IN" type="String" optional="false"/>
		
		
	</service>
